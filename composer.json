{
	"name": "consistence/consistence-doctrine-symfony",
	"description": "Symfony Bundle integrating Consistence library with Doctrine ORM",
	"keywords": ["consistence", "doctrine", "orm", "symfony", "bundle"],
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"php": "~7.2",
		"consistence/consistence-doctrine": "~1.2|~2.0",
		"doctrine/doctrine-bundle": "~1.3",
		"symfony/config": "~3.0|~4.0",
		"symfony/dependency-injection": "~3.0|~4.0",
		"symfony/http-kernel": "~3.0|~4.0",
		"symfony/yaml": "~3.0|~4.0"
	},
	"require-dev": {
		"consistence/coding-standard": "3.7",
		"php-parallel-lint/php-console-highlighter": "1.0",
		"php-parallel-lint/php-parallel-lint": "1.3.2",
		"phing/phing": "2.16.1",
		"phpunit/phpunit": "8.5.25"
	},
	"autoload": {
		"psr-4": {
			"Consistence\\Doctrine\\SymfonyBundle\\": "src"
		},
		"classmap": [ "src" ]
	},
	"autoload-dev": {
		"psr-4": {
			"Consistence\\Doctrine\\SymfonyBundle\\": "tests"
		},
		"classmap": [ "tests" ]
	},
	"config": {
		"bin-dir": "bin"
	}
}
